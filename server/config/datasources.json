{
  "memory": {
    "name": "memory",
    "connector": "memory"
  },
  "mongodb": {
    "host": "localhost",
    "port": 27017,
    "database": "db_shopkeepr",
    "name": "mongodb",
    "useNewUrlParser": true,
    "lazyConnect": true,
    "connector": "loopback-connector-mongodb"
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "database": 1,
    "name": "redis",
    "connector": "loopback-connector-redis"
  },
  "storage": {
    "name": "storage",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "maxFileSize": "10485760"
  }
}
