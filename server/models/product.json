{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "IdInject": {}
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "generated": false
    },
    "shopKeeperId": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "price": {
      "type": "object",
      "properties": [{
        "unit": {
          "type": "string"
        },
        "value": {
          "type": "number"
        }
      }]
    },
    "unit": {
      "type": "string"
    },
    "units": {
      "type": [
        "string"
      ]
    }
  },
  "validations": [],
  "relations": {
    "shop": {
      "type": "belongsTo",
      "model": "ShopKeeper",
      "foreignKey": "shopKeeperId"
    },
    "images": {
      "type": "hasMany",
      "model": "FileResource",
      "polymorphic": "uploadable"
    }
  },
  "acls": [],
  "methods": {}
}
